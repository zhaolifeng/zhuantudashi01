<!--pages/component/exportfile/exportfile.wxml-->
<view  class="exportfile" hidden="{{zindex}}">
   <view class="fileset">
       文件名
       <input name="fileName"/> 
       件格式
       <picker-view>
          <picker bindchange="" value="{{index}}" range="{{array}}">
            <view class="picker">
              当前选择：{{array[index]}} 
            </view>
          </picker>
       </picker-view>
 
   </view>
</view>
