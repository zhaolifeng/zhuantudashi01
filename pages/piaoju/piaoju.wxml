<!--pages/piaoju/piaoju.wxml-->
<scroll-view  style="height:{{height}}" scroll-y scroll-with-animation id="main0"  scroll-into-view="main-0}">
<checkbox-group bindchange="checkboxChange">
    <view id="resultData002002" wx:for="{{resultData}}" wx:for-index="idx" wx:for-item="itemName" s>
          <view wx:if="{{itemName.value !=''}}" style="padding-left:10px"> 
              <view class="line">
                <checkbox value="{{itemName.name}}" checked="{{itemName.checked}}">
                  {{itemName.name}}：<text selectable="true">{{itemName.value}}</text>
                </checkbox>
              </view>
          </view>
    </view>
</checkbox-group>
</scroll-view>
<view class="bottom">
<button type="primary" bindtap="selectAll">全选</button>
<button type="primary" bindtap="onCopyInfo">复制选中项</button>
</view>
